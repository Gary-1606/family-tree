{
  "presets": [
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": ["@babel/plugin-proposal-class-properties", ["module-resolver", {
    "root": ["."],
    "alias": {
      "@shared": "./src/shared",
      "@pages": "./src/pages"
    }
 }]],
 "env": {
  "test": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
}